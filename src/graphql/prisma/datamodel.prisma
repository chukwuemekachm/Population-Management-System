type User {
  id: ID! @unique
  firstName: String!
  lastName: String
  email: String! @unique
  password: String!
  locations: [Location!]
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Location {
  id: ID! @unique
  name: String!
  malePopulation: Int
  femalePopulation: Int
  parentLocation: Location @relation(name: "ParentLocation")
  subLocations: [Location!] @relation(name: "SubLocations")
  creator: User!
  createdAt: DateTime!
  updatedAt: DateTime!
}
